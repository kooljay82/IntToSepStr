{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true,
		"jest/globals": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"extends": [
		"eslint:recommended",
		"plugin:jsx-a11y/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
				"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"jsx-a11y",
		"jest"
	],
	"rules": {
	}
}
